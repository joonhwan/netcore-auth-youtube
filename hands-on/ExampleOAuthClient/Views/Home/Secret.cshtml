@{
    Layout = null;
}
<!DOCTYPE html>


<html>
<head>
    <title>Example-OAuthClient</title>
</head>
<body>
<h1>OAuth Client - Secret Page</h1>

<ul>
    <li>
        <a href="/home/authenticate">Authenticate</a>
    </li>
    <li>
        <a href="/home/login">LogIn</a>
    </li>
    <li>
        <a href="/home/register">Register</a>
    </li>
    <li>
        <a href="/home/secret">Secret</a>
    </li>
    <li>
        <a href="/home/logout">LogOut</a>
    </li>
</ul>

<h3>View Data</h3>
<ul>
    @{
        foreach (var item in ViewData)
        {
            <li>
                @item.Key : @item.Value
            </li>            
        }
    }
    
</ul>
</body>
</html>